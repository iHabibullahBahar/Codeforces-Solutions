#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        string str;
        cin>>str;
        int count=1;
        set <char> st;
        for(int i=0;i<str.length();i++)
        {
            st.insert(str[i]);
            if(st.size()>3)
            {
                st.clear();
                count++;
                i--;
            }
        }
        cout<<count<<endl;
    }
}
//checking it
